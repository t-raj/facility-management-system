<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
 
<hibernate-mapping package="com.facility.management.model.inspections">
	<class name="InspectionsImpl" table="inspections">
		<id name="inspectId" type="string" column="inspectId"/>
		<property name="facilityId" type="int" column="facilityId"/>
		<property name="description" type="string" column="description"/>
	</class>
	
</hibernate-mapping>